{
  "swagger": {
    "entryFile": "./build/routes.ts",
    "outputDirectory": "./build/swagger",
    "specMerging": "recursive",
    "spec": {
      "paths": {
        "/files/download": {
          "post": {
            "produces": [
              "image/png"
            ],
            "responses": {
              "200": {
                "description": "OK",
                "schema": {
                  "type": "string",
                  "format": "binary"
                }
              }
            }
          }
        }
      }
    },
    "tags":[
      {
        "name": "User",
        "description": "Operations about users",
        "externalDocs": {
          "description": "Find out more about users",
          "url": "http://swagger.io"
        }
      }
    ]
  },
  "routes": {
    "entryFile": "./src/controllers/index.ts",
    "routesDir": "./build",
    "basePath": "/service/",
    "authenticationModule": "./src/auth.ts",
    "middleware": "express",
    "iocModule": "./src/ioc"
  }
}
